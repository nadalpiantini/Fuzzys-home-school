{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "cache": false,
      "persistent": true
    },
    "@fuzzy/external-games#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@fuzzy/simulation-engine#build": {
      "dependsOn": [
        "@fuzzy/external-games#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@fuzzy/creative-tools#build": {
      "dependsOn": [
        "@fuzzy/external-games#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@fuzzy/vr-ar-adapter#build": {
      "dependsOn": [
        "@fuzzy/external-games#build"
      ],
      "outputs": [
        "dist/**"
      ]
    }
  }
}